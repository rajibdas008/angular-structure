{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1, 
  "newProjectRoot": "projects",
  "projects": {
    "gintaa": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/gintaa",
      "sourceRoot": "projects/gintaa/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/gintaa",
            "index": "projects/gintaa/src/index.html",
            "main": "projects/gintaa/src/main.ts",
            "polyfills": "projects/gintaa/src/polyfills.ts",
            "tsConfig": "projects/gintaa/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/gintaa/src/favicon.ico",
              "projects/gintaa/src/assets"
            ],
            "styles": [
              "projects/gintaa/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/gintaa/src/environments/environment.ts",
                  "with": "projects/gintaa/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "gintaa:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "gintaa:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "gintaa:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/gintaa/src/test.ts",
            "polyfills": "projects/gintaa/src/polyfills.ts",
            "tsConfig": "projects/gintaa/tsconfig.spec.json",
            "karmaConfig": "projects/gintaa/karma.conf.js",
            "assets": [
              "projects/gintaa/src/favicon.ico",
              "projects/gintaa/src/assets"
            ],
            "styles": [
              "projects/gintaa/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/gintaa/tsconfig.app.json",
              "projects/gintaa/tsconfig.spec.json",
              "projects/gintaa/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/gintaa/e2e/protractor.conf.js",
            "devServerTarget": "gintaa:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "gintaa:serve:production"
            }
          }
        }
      }
    },
    "gintaa-old": {
      "projectType": "application",
      "schematics":  {
        "@schematics/angular:component": {
