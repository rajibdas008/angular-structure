{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/app",
    "types": [],
    "paths": {
      "@gintaa/*": ["projecs/gintaa-old/src/app/*"],
      "@gintaa/service/*": ["projecs/gintaa-old/src/app/common/service/*"],
